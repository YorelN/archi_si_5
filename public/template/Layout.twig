        <ul>
            <li><a href="#"><img src="../assets/images/img-layout/fb.png"></a></li>
            <li><a href="#"><img src="../assets/images/img-layout/insta.png"></a></li>
            <li><a href="#"><img src="../assets/images/img-layout/pinterest.png"></a></li>
            <li><a href="#"><img src="../assets/images/img-layout/twitter.png"></a></li>
        </ul>
    </div>
    <div class="bas-de-page">
        <div>
            <h5 class="archi"><a href="#">L'Architecture</a></h5>
        </div>
        <div>
            <h5 class="legales"><a href="inde">Mentions légales</a></h5>
